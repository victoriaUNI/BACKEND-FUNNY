{
  "version": 2,
  "builds": [
    {
      "src": "vercel.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**", ".env"],
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}